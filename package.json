{
  "name": "react-app-boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development NODE_OPTIONS='--openssl-legacy-provider --max-old-space-size=8192' ./node_modules/.bin/webpack-dev-server",
    "build": "cross-env NODE_ENV=production NODE_OPTIONS='--openssl-legacy-provider --max-old-space-size=8192' ./node_modules/.bin/webpack",
    "build:zip": "npm run build && cd dist && zip -r app.zip ./*",
    "test": "jest",
    "lint": "eslint --no-error-on-unmatched-pattern ./src/* --ext .js,.jsx,.ts,.tsx",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "core-js": "^3.21.0",
    "dotenv": "^16.0.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.1",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "stylelint": "^14.9.1",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-scss": "^4.2.0",
    "@babel/core": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.17.0",
    "@svgr/webpack": "^6.2.1",
    "@types/react": "^18.0.14",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.5.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.4.1",
    "postcss": "^8.4.6",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.3.1",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
